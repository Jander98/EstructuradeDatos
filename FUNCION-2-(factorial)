#include "stdafx.h"
#include <iostream> 
#include "conio.h"
#include "math.h"

using namespace std;

int serie(int n);
void main()
{int n,s=0;
cout<<"ingrese el limite deL FACTORIAL"<<endl;
cin>>n;
s=serie(n);
cout<<"el resultado deL FACTORIAL es:"<<s<<endl;
getch();
}

int serie(int n)
{int s=1;
    for(int i=1;i<=n;i++)
	  s=s*i;
return s;
}
